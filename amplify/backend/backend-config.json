{
	"storage": {
		"todolist": {
			"service": "DynamoDB",
			"providerPlugin": "awscloudformation"
		},
		"members": {
			"service": "DynamoDB",
			"providerPlugin": "awscloudformation"
		}
	},
	"function": {
		"LambdaToDoList": {
			"build": true,
			"providerPlugin": "awscloudformation",
			"service": "Lambda",
			"dependsOn": [
				{
					"category": "storage",
					"resourceName": "todolist",
					"attributes": [
						"Name",
						"Arn"
					],
					"attributeEnvMap": {
						"Name": "TABLE_NAME",
						"Arn": "TABLE_ARN"
					}
				}
			]
		},
		"LambdaMembers": {
			"build": true,
			"providerPlugin": "awscloudformation",
			"service": "Lambda",
			"dependsOn": [
				{
					"category": "storage",
					"resourceName": "members",
					"attributes": [
						"Name",
						"Arn"
					],
					"attributeEnvMap": {
						"Name": "TABLE_NAME",
						"Arn": "TABLE_ARN"
					}
				}
			]
		}
	},
	"api": {
		"ApiToDoList": {
			"service": "API Gateway",
			"providerPlugin": "awscloudformation",
			"dependsOn": [
				{
					"category": "function",
					"resourceName": "LambdaToDoList",
					"attributes": [
						"Name",
						"Arn"
					]
				}
			]
		},
		"ApiMembers": {
			"service": "API Gateway",
			"providerPlugin": "awscloudformation",
			"dependsOn": [
				{
					"category": "function",
					"resourceName": "LambdaMembers",
					"attributes": [
						"Name",
						"Arn"
					]
				}
			]
		}
	},
	"hosting": {
		"amplifyhosting": {
			"service": "amplifyhosting",
			"providerPlugin": "awscloudformation",
			"type": "manual"
		}
	}
}