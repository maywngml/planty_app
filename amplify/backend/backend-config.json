{
    "storage": {
        "memebers": {
            "service": "DynamoDB",
            "providerPlugin": "awscloudformation"
        }
    },
    "function": {
        "memebersLambda": {
            "build": true,
            "providerPlugin": "awscloudformation",
            "service": "Lambda",
            "dependsOn": [
                {
                    "category": "storage",
                    "resourceName": "memebers",
                    "attributes": [
                        "Name",
                        "Arn"
                    ],
                    "attributeEnvMap": {
                        "Name": "TABLE_NAME",
                        "Arn": "TABLE_ARN"
                    }
                }
            ]
        }
    },
    "api": {
        "membersApi": {
            "service": "API Gateway",
            "providerPlugin": "awscloudformation",
            "dependsOn": [
                {
                    "category": "function",
                    "resourceName": "memebersLambda",
                    "attributes": [
                        "Name",
                        "Arn"
                    ]
                }
            ]
        }
    },
    "hosting": {
        "amplifyhosting": {
            "service": "amplifyhosting",
            "providerPlugin": "awscloudformation",
            "type": "manual"
        }
    }
}